<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.MainActivity"
        android:layout_marginLeft="12sp"
        android:layout_marginTop="10sp">
        <LinearLayout
            android:id="@+id/linearLayoutStoryLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5sp">
            <RelativeLayout
                android:id="@+id/storyRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/storyLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Life Story Lines"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showStory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/storyLine"
                    tools:text="SHOW LIFE STORY LINES" />
            </RelativeLayout>
            <Switch
                android:id="@+id/storyLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutFamilyLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutStoryLine">
            <RelativeLayout
                android:id="@+id/familyRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/familyLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Family Tree Lines"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showFamily"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/familyLine"
                    tools:text="SHOW FAMILY TREE LINES" />
            </RelativeLayout>
            <Switch
                android:id="@+id/familyLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutSpouseLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutFamilyLine">
            <RelativeLayout
                android:id="@+id/SpouseRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/SpouseLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Spouse Lines"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showSpouse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/SpouseLine"
                    tools:text="SHOW SPOUSE LINES" />
            </RelativeLayout>
            <Switch
                android:id="@+id/SpouseLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutFatherLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutSpouseLine">
            <RelativeLayout
                android:id="@+id/FatherRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/FatherLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Father's Side"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showFather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/FatherLine"
                    tools:text="FILTER BY FATHER'S SIDE OF FAMILY" />
            </RelativeLayout>
            <Switch
                android:id="@+id/FatherLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutMotherLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutFatherLine">
            <RelativeLayout
                android:id="@+id/MotherRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/MotherLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mother's Side"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showMother"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/MotherLine"
                    tools:text="FILTER BY MOTHER'S SIDE OF FAMILY" />
            </RelativeLayout>
            <Switch
                android:id="@+id/MotherLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutMaleLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutMotherLine">
            <RelativeLayout
                android:id="@+id/MaleRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/MaleLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Male Events"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showMale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/MaleLine"
                    tools:text="FILTER EVENTS BASED ON GENDER" />
            </RelativeLayout>
            <Switch
                android:id="@+id/MaleLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutFemaleLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/linearLayoutMaleLine">
            <RelativeLayout
                android:id="@+id/FemaleRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8">
                <TextView
                    android:id="@+id/FemaleLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Female Events"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/showFemale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/FemaleLine"
                    tools:text="FILTER EVENTS BASED ON GENDER" />
            </RelativeLayout>
            <Switch
                android:id="@+id/FemaleLineSwitch"
                android:layout_width="52dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="false">
            </Switch>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutLogout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="8sp"
            android:layout_below="@+id/linearLayoutFemaleLine"
            android:background="@android:color/transparent">
            <RelativeLayout
                android:id="@+id/LogoutRelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="8"
                android:clickable="true">
                <TextView
                    android:id="@+id/Logout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Logout"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/showLogout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Logout"
                    tools:text="RETURN TO LOGIN SCREEN" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

